//       DM.ACBrNFe1.Configuracoes.Arquivos.Pa                 :=  FieldByName( 'NFCE_PASTA_TRANSMITIDAS' ).AsString;
         if trim(FieldByName( 'ID_TOKEN' ).AsString) <> '' then
            begin
